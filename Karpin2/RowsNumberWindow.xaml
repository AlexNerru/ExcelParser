<Window x:Class="Karpin2.RowsNumberWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Karpin2"
        mc:Ignorable="d"
        Title="RowsNumberWindow" Height="189.8" Width="300" Topmost="True">
    <Grid Margin="0,0,0.2,12.4">
        <Label Content="Введите холичество строк для демонстрации:" HorizontalAlignment="Left" Margin="10,34,0,0" VerticalAlignment="Top" Width="270"/>
        <TextBox x:Name="RowsTextBox" HorizontalAlignment="Left" Height="23" Margin="20,107,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="SelectRowsButton" Content="Вперед" HorizontalAlignment="Left" Margin="185,107,0,0" VerticalAlignment="Top" Width="75" Height="23"/>

    </Grid>
</Window>
